{
  "name": "material-ui-popup-state",
  "version": "5.1.2",
  "description": "easiest way to create menus, popovers, and poppers with material-ui",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jcoreio/material-ui-popup-state.git"
  },
  "keywords": [
    "material-ui",
    "popup",
    "menu",
    "popover",
    "boilerplate",
    "render-props",
    "react-hooks"
  ],
  "author": "Andy Edwards",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcoreio/material-ui-popup-state/issues"
  },
  "homepage": "https://github.com/jcoreio/material-ui-popup-state#readme",
  "dependencies": {
    "@babel/runtime": "^7.20.6",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^18.0.26",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "@mui/material": "^5.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "default": "./index.js"
    },
    "./index": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "default": "./index.js"
    },
    "./hooks": {
      "types": "./hooks.d.ts",
      "import": "./hooks.mjs",
      "default": "./hooks.js"
    },
    "./HoverMenu": {
      "types": "./HoverMenu.d.ts",
      "import": "./HoverMenu.mjs",
      "default": "./HoverMenu.js"
    },
    "./HoverPopover": {
      "types": "./HoverPopover.d.ts",
      "import": "./HoverPopover.mjs",
      "default": "./HoverPopover.js"
    },
    "./chainEventHandlers": {
      "types": "./chainEventHandlers.d.ts",
      "import": "./chainEventHandlers.mjs",
      "default": "./chainEventHandlers.js"
    }
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@8.11.0"
}
